install="${pkgname}.install"
source=('Makefile'
        'adk.gpg'
	'adk-revoked'
	'adk-trusted')
validpgpkeys=('360DCBA5948FDE5944CA1E2E1A8DE33B5A149FDF')
md5sums=('dd200885a6a4464c05673c35c0585dea'
         '658fa0e2d4eb31606fa30ee42e65ef7d'
         'c7ebce76fad9770303c4910a768a7b15'
         '9c83e4662cccc6692fd0ae51061e58d7')

package() {
	cd "${srcdir}"
	make PREFIX=/usr DESTDIR=${pkgdir} install
}
